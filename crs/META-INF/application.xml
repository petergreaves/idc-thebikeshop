<?xml version="1.0"?>
<application xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd" version="6">
  <display-name>crs</display-name>
  <initialize-in-order>true</initialize-in-order>
  <module>
    <web>
      <web-uri>crs-custom-web.war</web-uri>
      <context-root>/shop</context-root>
    </web>
  </module>
  <module>
    <web>
      <web-uri>crs-ibm-web.war</web-uri>
      <context-root>/webapp/remote/ibmstores</context-root>
    </web>
  </module>
  <module>
    <web>
      <web-uri>crs-preview-web.war</web-uri>
      <context-root>/webapp/remote/preview</context-root>
    </web>
  </module>
  <module>
    <web>
      <web-uri>crs-alias-proxy-web.war</web-uri>
      <context-root>/wcsstore</context-root>
    </web>
  </module>
  <module>
    <web>
      <web-uri>crs-ibm-alias-proxy-web.war</web-uri>
      <context-root>/ibmstorealias</context-root>
    </web>
  </module>
  <module>
    <web>
      <web-uri>crs-health-check-web.war</web-uri>
      <context-root>/health/resources</context-root>
    </web>
  </module>
  <library-directory>lib</library-directory>
</application>
