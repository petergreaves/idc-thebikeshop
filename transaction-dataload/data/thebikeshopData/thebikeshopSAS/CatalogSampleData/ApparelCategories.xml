<!-- Add Categories -->

<!-- Define the Apparel Top categories. These display on the home page and the stores header and/or sidebar -->
<catgroup 
   catgroup_id="@catgroup_id_apparel"
   member_id="&CAT_MEMBER_ID;"
   identifier="Apparel"
   markfordelete="0"
/>

<!-- Identifies the root CatalogGroups (or root Categories) contained within a Catalog -->
<!-- The above Apparel categories exist at the root of the Catalog                           -->
<cattogrp
   catalog_id="&CATALOG_ID;"
   catgroup_id="@catgroup_id_apparel"
   sequence="4"
/>

<!-- Relate the categories to the store -->
<storecgrp
   storeent_id="&CAT_STOREENT_ID;"
   catgroup_id="@catgroup_id_apparel"
/>

<!-- Start new category for Aurora -->

